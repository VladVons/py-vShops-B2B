{
    "dir_data": "./Data/Tenant",
    "comment": "Export price lists",
    "include": [
        "dbl.json",
        "pcdata.json",
        "mdm.json",
        "mdm_t.json",
        "misc.json"
    ],
    "plugins": [
        "-In_oster-tk22",
        "Out_vShopTenant_db"
    ],
    "common": {
        "sleep_loop": 0.01,
        "save_cache": true
    },
    "plugin": {
        "Out_vShopTenant_db": {
            "sql": {
                "parts": 100,
                "lang": "ua"
            },
            "auth": "{% file Conf/Task.SrvModel.Api.json:db_auth %}",
            "depends": [
                "-In_comelit",
                "-In_pcdata",
                "-In_mdm",
                "In_mdm_t",
                "-In_oster-tk22",
                "-In_oster-tz20",
                "-In_oster-tr21"
            ],
            "img_loader": "{% file Conf/Task.SrvCtrl.Api.json:loader.img %}",
            "img_conf": "{% file Conf/Task.SrvImg.Api.json %}"
        }
    }
}
