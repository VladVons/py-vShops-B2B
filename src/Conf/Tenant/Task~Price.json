{
    "dir_data": "./Data/Tenant",
    "comment": "Export price lists",
    "include": [
        "boda.json",
        "pcdata.json",
        "mdm.json",
        "drogobich.json",
        "-uttc.json"
    ],
    "plugins": [
        "Out_vShopTenant_db",
        "-Out_vShopNoDescr_db",
        "-Out_Save_txt",
        "-Out_Save_xlsx"
    ],
    "common": {
        "sleep_loop": 0.01,
        "save_cache": false
    },
    "plugin": {
        "Out_vShopTenantModelUnknown_db" : {
            "auth": "{% file Conf/Task.SrvModel.Api.json:db_auth %}"
        },
        "Out_vShopTenant_db": {
            "sql": {
                "parts": 10,
                "lang": "ua"
            },
            "auth": "{% file Conf/Task.SrvModel.Api.json:db_auth %}",
            "depends": [
                "-In_boda",
                "-In_pcdata",
                "In_mdm",
                "-In_drogobich",
                "-In_uttc"
            ],
            "img_loader": "{% file Conf/Task.SrvCtrl.Api.json:loader.img %}",
            "img_conf": "{% file Conf/Task.SrvImg.Api.json %}"
        },
        "Out_vShopNoDescr_db": {
            "sql": {
                "parts": 10,
                "lang": "ua",
                "tenant": "pcdata",
                "categories": ["Ноутбук", "Комп'ютер"]
            },
            "auth": "{% file Conf/Task.SrvModel.Api.json:db_auth %}"
        },
        "Out_Save_txt" : {
            "depends": [
                "-In_mdm",
                "-In_pcdata",
                "-In_uttc",
                "-Out_vShopTenantModelUnknown_db"
            ],
            "db_list": {
                "len": 80
            }
        },
        "Out_Save_xlsx" : {
            "depends": [
                "Out_vShopTenantModelUnknown_db"
            ]
        }
    }
}
