{
    "dir_data": "./Data/Product0",
    "comment": "Grab EAN, IceCat etc",
    "include": [
        "vendors.json"
    ],
    "plugins": [
        "-Out_vShopCrawl_db",
        "Out_vShopTenantModel_db"
    ],
    "common": {
        "sleep_loop": 0.01,
        "save_cache": true
    },
    "plugin": {
        "Out_vShopCrawl_db": {
            "sql": {
                "parts": 10,
                "lang": "ua",
                "max_conn": 1,
                "rand_sleep": [0.1, 1.1],
                "parser": "icecat_biz"
            },
            "auth": "{% file Conf/Task.SrvModel.Api.json:db_auth %}",
            "depends": [
                "-In_Crawl_ean0_txt",
                "-In_Crawl_ean_txt",
                "-In_Crawl_kalina_xls",
                "-In_Crawl_fozzy_sqlite",
                "In_Crawl_icecat_xlsx"
            ],
            "img_loader": "{% file Conf/Task.SrvCtrl.Api.json:loader.img %}",
            "img_conf": "{% file Conf/Task.SrvImg.Api.json %}"
        },
        "Out_vShopTenantModel_db": {
            "sql": {
                "lang": "ua",
                "tenant": "mdm",
                "parts": 50
            },
            "auth": "{% file Conf/Task.SrvModel.Api.json:db_auth %}",
            "depends": [
                "Out_vShopCrawl_db",
                "In_Crawl_icecat_xlsx"
            ]
        }
    }
}
